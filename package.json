{
  "name": "postgraphile-form",
  "version": "1.0.4",
  "description": "A Graphql Form for PostGraphile",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest src",
    "coverage": "jest src --coverage",
    "build": "webpack --mode development",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "deploy-storybook": "storybook-to-ghpages",
    "start": "start-storybook -p 80",
    "storybook": "start-storybook -p 9001",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "apollo:codegen": "apollo client:codegen --target=typescript --passthroughCustomScalars --customScalarsPrefix GraphQL",
    "release-it": "yarn version --patch && git push --follow-tags"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/(index|stories).{js,jsx,ts,tsx}",
      "!src/components/Table/**/*",
      "!src/**/(__generated__|__tests__|__mock__|__test__)/*.{js,jsx,ts,tsx}"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/src/__mocks__/styleMock.js",
      "@/(.*)": "<rootDir>/src/$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/.storybook/"
    ],
    "automock": false,
    "testURL": "http://localhost",
    "setupFiles": [
      "<rootDir>/src/__test__/setup.js"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.tsx?$": "ts-jest"
    },
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hansololai/graphql_form"
  },
  "keywords": [
    "postgraphileForm",
    "graphqlForm",
    "react",
    "shared",
    "components"
  ],
  "author": "Han Lai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hansololai/graphql_form/issues"
  },
  "homepage": "https://github.com/hansololai/graphql_form",
  "greenkeeper": {
    "ignore": [
      "react-apollo"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@emotion/cache": "^10.0.0",
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@emotion/utils": "^0.11.1",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-info": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@storybook/storybook-deployer": "^2.8.1",
    "@testing-library/react": "9.0.1",
    "@types/jest": "^24.0.17",
    "@types/storybook__react": "^4.0.1",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.15",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^3.2.0",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-junit": "^6.2.1",
    "jest-transform-graphql": "^2.1.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "node-sass": "^4.11.0",
    "react-docgen-typescript-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "waait": "^1.0.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.2.0"
  },
  "dependencies": {
    "antd": "^3.18.1",
    "change-case": "^3.1.0",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "pluralize": "^8.0.0",
    "react": "^16.9.0",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.9.0",
    "styled-components": "^4.1.3",
    "use-debounce": "^2.0.2"
  }
}
